source ~/.vim/vimrc 
call pathogen#runtime_append_all_bundles() 
call pathogen#helptags()

colorscheme ir_black


map <F5> :NERDTreeToggle<cr>

set number

set guifont=Consolas:h12

set tabstop=2
set smarttab
set shiftwidth=2
set autoindent
set expandtab

set grepprg=ack


set nobackup
set nowritebackup
set noswapfile

